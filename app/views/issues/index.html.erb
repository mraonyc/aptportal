<h1>Issues</h1>

<% @issues.each do |issue| %>

<table> 
 <tr>	
    <td><%= link_to issue.name, issue %></td>
    <td><%= issue.apartment.name %></td>
    
    <td><%= issue.category %></td>
    <td><%= issue.issue_date %></td>
  </tr>
<% end %>
</table>

<br />
<div class="clear">
<%= link_to 'New Issue', new_issue_path %>
</div>